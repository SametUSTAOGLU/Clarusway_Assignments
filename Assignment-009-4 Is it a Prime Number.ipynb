   "source": [
    "number = int(input(\"Enter a number for check is it s prime number: \"))\n",
    "first = 2\n",
    "for i in range(number-2):\n",
    "    if number % first == 0:\n",
    "        print(f\"{number} is not a prime number\")\n",
    "        break\n",
    "    elif first == number-1:\n",
    "        print(f\"{number} is a prime number\")\n",
    "    first += 1"
   ]
